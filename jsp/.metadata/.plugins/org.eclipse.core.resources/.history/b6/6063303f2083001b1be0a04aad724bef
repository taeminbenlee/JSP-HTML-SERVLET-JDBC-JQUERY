<script>
$(function(){
	
	$("button").click(function(){
		
		$.ajax({
			url:"books.xml",
			dataType: "xml",
			success: function(xml) {
                $(xml).find('xmldata').each(function(){
                    $(this).find("book").each(function(){
                    	
                        //var values = $(this).text();
                        //alert(values);
                        $("#bookss").append($(this).find("#bk101").text() + "<br>");
                        $("#bookss").append($(this).find("#bk102").text() + "<br>");
                        $("#bookss").append($(this).find("#bk103").text() + "<br>");
                    });
                });
            }
		});
		
		
		
	});
	
});

</script>



