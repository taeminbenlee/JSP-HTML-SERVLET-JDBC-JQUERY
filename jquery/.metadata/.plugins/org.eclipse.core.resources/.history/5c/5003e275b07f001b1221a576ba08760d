<img alt="" src="./images/photo_a.jpg">
</body>
<script type="text/javascript">
$(document).ready(function(){
	let num = 0;
    $("img").click(function(){
        // Change src attribute of image
        if(num==1){
        $(this).attr("src", "./images/photo_b.jpg");
        num==1;
        }else{ //원상복귀를 위해 num변수를 지정
        	$(this).attr("src", "./images/photo_a.jpg");
        	num=0;
        }
    });    
});
</script>
