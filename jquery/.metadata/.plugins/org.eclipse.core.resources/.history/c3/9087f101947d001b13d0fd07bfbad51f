<script type="text/javascript">
let man = [
	["",""],
	["홍길동","01:02:81"],
	["일지매","01:05:74"],
	["임꺽정","01:07:93"],
	["김철수","01:09:93"],
	["이을용","01:11:23"]
];
let woman = [
	["",""],
	["김영희","01:06:11"],
	["성춘향","01:11:34"],
	["박주희","01:13:57"],
	["이영애","01:14:34"],
	["전지현","01:19:34"]
];
$(document).ready(function(){
	$("#woman").click(function(){
		for (i = 0; i < 6; i++) {
			for (j = 0; j < 2; j++) {
				$("tr:eq("+i+") td:eq(" +j+ ")").html(woman[i][j]);
			}
		}
		
	});
	$("#man").click(function(){
		for (i = 0; i < 6; i++) {
			for (j = 0; j < 2; j++) {
				$("tr:eq("+i+") td:eq(" +j+ ")").html(man[i][j]);
			}
		}
		
	});
	$("td").mousedown(function() {
		alert(this)
	})
});
</script>
