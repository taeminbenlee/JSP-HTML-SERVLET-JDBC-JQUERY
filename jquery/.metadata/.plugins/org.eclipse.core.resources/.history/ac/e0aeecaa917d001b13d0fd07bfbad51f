<script type="text/javascript">
var image = [
	"photo_1.jpg","photo_2.jpg", "photo_3.jpg","photo_4.jpg","photo_5.jpg",
"photo_6.jpg", "photo_7.jpg", "photo_8.jpg", "photo_9.jpg" ];

$(document).ready(function(){
	//$( "tr:eq(1) td:eq(1)" ).html("데이터");
	$('#append').click(function () {
		for (i = 0; i < arr_image.length; i++) {
			$('#example2').append("<td>+image[i]+<td>");
			
			}
		}
		
	});
        
    
    


});
</script>
